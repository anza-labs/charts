{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "affinity": {
      "type": "object"
    },
    "config": {
      "type": "object",
      "properties": {
        "manageHttpPort": {
          "type": "integer"
        },
        "manageHttpsPort": {
          "type": "integer"
        },
        "pgid": {
          "type": "integer"
        },
        "pgroup": {
          "type": "string"
        },
        "portAdoptV1": {
          "type": "integer"
        },
        "portAppDiscovery": {
          "type": "integer"
        },
        "portDiscovery": {
          "type": "integer"
        },
        "portManagerV1": {
          "type": "integer"
        },
        "portManagerV2": {
          "type": "integer"
        },
        "portRtty": {
          "type": "integer"
        },
        "portTransferV2": {
          "type": "integer"
        },
        "portUpgradeV1": {
          "type": "integer"
        },
        "portalHttpPort": {
          "type": "integer"
        },
        "portalHttpsPort": {
          "type": "integer"
        },
        "puid": {
          "type": "integer"
        },
        "pusername": {
          "type": "string"
        },
        "rootless": {
          "type": "boolean"
        },
        "showMongoDbLogs": {
          "type": "boolean"
        },
        "showServerLogs": {
          "type": "boolean"
        },
        "skipUserlandKernelCheck": {
          "type": "boolean"
        },
        "smallFiles": {
          "type": "boolean"
        },
        "timezone": {
          "type": "string"
        },
        "tlsLegacyEnabled": {
          "type": "boolean"
        }
      }
    },
    "fullnameOverride": {
      "type": "string"
    },
    "image": {
      "type": "object",
      "properties": {
        "pullPolicy": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      }
    },
    "imagePullSecrets": {
      "type": "array"
    },
    "nameOverride": {
      "type": "string"
    },
    "nodeSelector": {
      "type": "object"
    },
    "persistence": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "accessMode": {
              "type": "string"
            },
            "annotations": {
              "type": "object"
            },
            "enabled": {
              "type": "boolean"
            },
            "existingClaim": {
              "type": "string"
            },
            "size": {
              "type": "string"
            },
            "storageClass": {
              "type": "string"
            }
          }
        }
      }
    },
    "podAnnotations": {
      "type": "object"
    },
    "podLabels": {
      "type": "object"
    },
    "podSecurityContext": {
      "type": "object"
    },
    "resources": {
      "type": "object"
    },
    "securityContext": {
      "type": "object"
    },
    "service": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        }
      }
    },
    "serviceAccount": {
      "type": "object",
      "properties": {
        "annotations": {
          "type": "object"
        },
        "automount": {
          "type": "boolean"
        },
        "create": {
          "type": "boolean"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "tolerations": {
      "type": "array"
    },
    "updateStrategy": {
      "type": "object",
      "properties": {
        "rollingUpdate": {
          "type": "object",
          "properties": {
            "maxUnavailable": {
              "type": "string"
            },
            "partition": {
              "type": "integer"
            }
          }
        },
        "type": {
          "type": "string"
        }
      }
    }
  }
}
